{
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://box-backend-otsb.onrender.com/api/$1"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        }
      ]
    }
  ]
}