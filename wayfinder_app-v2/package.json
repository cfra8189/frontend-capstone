{
  "name": "wayfinderos",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "tsx watch server/index.ts",
    "dev:client": "vite",
    "build": "vite build && esbuild server/index.ts --bundle --platform=node --format=esm --outfile=dist/server/index.js --packages=external",
    "start": "node dist/server/index.js"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.0.0",
    "bcryptjs": "^2.4.3",
    "connect-mongo": "^5.1.0",
    "dotenv": "^16.3.1",
    "express": "^4.19.2",
    "express-session": "^1.18.0",
    "memoizee": "^0.4.17",
    "mongoose": "^8.0.0",
    "openid-client": "^6.1.3",
    "passport": "^0.7.0",
    "passport-github2": "^0.1.12",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "resend": "^4.0.0",
    "wouter": "^3.0.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.18.0",
    "@types/memoizee": "^0.4.11",
    "@types/node": "^20.10.0",
    "@types/passport": "^1.0.16",
    "@types/passport-github2": "^1.2.9",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^4.2.0",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.4.0",
    "esbuild": "^0.20.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0"
  }
}
